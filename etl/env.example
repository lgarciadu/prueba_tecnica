# Archivo de configuración de ejemplo para el ETL de datos climáticos
# Copia este archivo a .env y completa los valores reales

# Configuración de base de datos MySQL
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASSWORD=rootpass
DB_NAME=testdb

# Configuración de API meteorológica
API_KEY=REPLACE_ME
API_BASE=https://api.openweathermap.org/data/2.5/weather

# Configuración de procesamiento
MAX_WORKERS=8
REQUEST_TIMEOUT=30
MAX_RETRIES=3

# Configuración de logging
LOG_LEVEL=INFO
